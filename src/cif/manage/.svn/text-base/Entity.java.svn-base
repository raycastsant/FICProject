package cif.manage;

/**
 *
 * @author Pedro
 */
public abstract class Entity
{
    protected String id;
    protected String name;
    protected Entity(String id, String nombre)
    {
        this.id = id;
        name = nombre;
    }

    public String getID()
    {
        return id;
    }

    public String getName()
    {
        return name;
    }
}
