        // TODO add your handling code here:
      Integer cantrow=DBAccess.verificarPkey(id, anno, tablename);
        if (cantrow==0)
        {
            DBAccess.insertInc(id,anno, cant, distrec,superftot, tablename);
            DBAccess.insertIncEsp(id,anno,idespecie, perddir,perdind,haperd, tablenameEsp)
        }
        else
        {
          DBAccess.insertIncEsp(id,anno,idespecie, perddir,perdind,haperd, tablenameEsp);
        }
    }//GEN-LAST:event_jButton_AceptarActionPerformed


