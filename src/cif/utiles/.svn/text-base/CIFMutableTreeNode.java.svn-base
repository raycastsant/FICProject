package cif.utiles;

import javax.swing.tree.DefaultMutableTreeNode;

/**
 *
 * @author Pedro
 */
public class CIFMutableTreeNode extends DefaultMutableTreeNode
{
    private Manejable manage_obj;
    public CIFMutableTreeNode(Object object, Manejable manage)
    {
        super(object);
        manage_obj = manage;
    }

    public void setManageObject(Manejable manage)
    {
        manage_obj = manage;
    }

    public Manejable getManageObject()
    {
        return manage_obj;
    }
}
